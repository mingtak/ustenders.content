<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="ustenders.content">
<body>

<metal:content-core fill-slot="content-core">
<metal:content-core define-macro="content-core"
                    tal:define="toc context/table_of_contents|nothing;">

<tal:naics define="catalog python:context.portal_catalog;
                   naicsLen python:len(context.naicsCode)"
           condition="python:naicsLen < 6">
    <tal:brain define="brain view/get_next_level">
<h5 tal:repeat="item brain">
  <a tal:content="item/Title" tal:attributes="href item/getURL"></a>
</h5>
    </tal:brain>
</tal:naics>



<tal:back_reference define="items view/get_back_references">
  <ul>
    <li tal:repeat="item items">
      <a tal:attributes="href item/absolute_url" tal:content="item/Title" />
    </li>
  </ul>
</tal:back_reference>

</metal:content-core>
</metal:content-core>

</body>
</html>
